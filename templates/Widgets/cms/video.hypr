{% if not Model.config %} 
    <div class="mz-cms-image-cover" style="background-image: url('/admin/scripts/resources/images/legacy/AddPhotos.png');"></div>
{% else %}
    <div class="mz-cms-video-placeholder">
         <iframe width="{{ Model.config.width }}" height="{{ Model.config.height }}" src="{{ Model.config.src }}" frameborder="0" allowfullscreen></iframe>
    </div>
{% endif %}